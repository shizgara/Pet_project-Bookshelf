[program:pythonapp]
directory=/opt/app/bookshelf-gurskyi/gce/
command=/opt/app/bookshelf-gurskyi/gce/env/bin/honcho start -f ./procfile worker bookshelf
autostart=true
autorestart=true
user=pythonapp
# Environment variables ensure that the application runs inside of the
# configured virtualenv.
environment=VIRTUAL_ENV="/opt/app/bookshelf-gurskyi/gce/env",PATH="/opt/app/bookshelf-gurskyi/gce/env/bin", HOME="/home/pythonapp",USER="pythonapp"
stdout_logfile=syslog
stderr_logfile=syslog